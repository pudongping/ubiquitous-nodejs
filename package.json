{
  "name": "ubiquitous-nodejs",
  "version": "1.0.0",
  "description": "ubiquitous-nodejs is a web scaffold which is based on node.js",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development node --use_strict app.js",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node --use_strict app.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node --use_strict app.js",
    "test": "cross-env NODE_ENV=test node --use_strict app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pudongping/ubiquitous-nodejs.git"
  },
  "keywords": [
    "koa",
    "async",
    "nunjucks"
  ],
  "author": "Alex Pu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pudongping/ubiquitous-nodejs/issues"
  },
  "homepage": "https://github.com/pudongping/ubiquitous-nodejs#readme",
  "dependencies": {
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "koa": "^2.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "mime": "^2.5.2",
    "mysql": "^2.18.1",
    "mz": "^2.7.0",
    "nunjucks": "^3.2.3",
    "sequelize": "^6.6.2"
  }
}
